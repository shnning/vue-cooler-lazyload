{
  "version": "1.0.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "dev": "rm -rf dist && concurrently \"npm start\" \"wait-on dist/index.js && webpack serve --config example/build/webpack.config.js\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shnning/vue-cooler-lazyload.git"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "vue-cooler-lazyload",
  "author": "sh.ning",
  "module": "dist/vue-cooler-lazyload.esm.js",
  "size-limit": [
    {
      "path": "dist/vue-cooler-lazyload.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/vue-cooler-lazyload.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@size-limit/preset-small-lib": "^4.9.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.6",
    "size-limit": "^4.9.1",
    "style-loader": "^2.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.5",
    "vue-template-compiler": "^2.6.12",
    "wait-on": "^5.2.0",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "keywords": [
    "vue",
    "image",
    "img",
    "lazyload"
  ]
}
